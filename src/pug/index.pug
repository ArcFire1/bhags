                        i.stars__out: i.stars__in(style="width: 100%;")
                        i.stars__out: i.stars__in(style="width: 100%;")
                        i.stars__out: i.stars__in(style="width: 50%;")
                    h5.speaker__name Гранд Кардон
                    p.speaker__translate Grande Cardone
                    p.speaker__info Инвестор, строительный магнат
                    a.button.button_more Подробнее
              .cell.medium-auto
                .speaker(data-equalizer-watch="speaker")
                  .speaker__photo(style="background-image: url(./i/speaker.jpg)")
                  .speaker__content
                    .speaker__rating
                      span.speaker__average 4.5
